NGINX_DOMAIN_NAME=warriorslife.site

NGINX_REPORT_URL=https://warriorslife.site/violation_reports

NGINX_RESOLVER=127.0.0.1

NGINX_BACKEND=backend:8191
NGINX_WEBSOCKETS=[["chat","^/websocket/chat$","backend:8192"]]

NGINX_ERROR_BAD_REQUEST=/errors/bad_request.html
NGINX_ERROR_FORBIDDEN=/errors/forbidden.html
NGINX_ERROR_NOT_FOUND=/errors/not_found.html
NGINX_ERROR_TOO_LARGE=/errors/too_large.html
NGINX_ERROR_TOO_EARLY=/errors/too_early.html
NGINX_ERROR_TOO_MANY_REQUESTS=/errors/too_many_requests.html
NGINX_ERROR_INTERNAL=/errors/internal.html
NGINX_ERROR_SERVICE_UNAVAILABLE=/errors/service_unavailable.html

NGINX_CERT=/ssl/warriorslife.site.crt
NGINX_CERT_KEY=/ssl/warriorslife.site.key
NGINX_TRUSTED_CERTS=/ssl/warriorslife.site.fullchain.crt
NGINX_DH_PARAMS=/ssl/dhparam.pem

NGINX_DUMPS=/dumps
NGINX_STATIC=/static

NGINX_AUTH_REALM=off
NGINX_AUTH_USERS=/nginx.htpasswd

# TODO: determine best values for some of these? (TEST, TWEAK)
# TODO: ...or at least set their defaults in files (FEAT)
# all *timeout values can pretty much be left at their defaults

NGINX_WORKER_RLIMIT_NOFILE=2048
NGINX_WORKER_RLIMIT_CORE=16G

NGINX_WORKER_PROCESSES=auto
NGINX_WORKER_CONNECTIONS=1024
NGINX_WORKER_AIO_REQUESTS=32

# on or off
NGINX_MULTI_ACCEPT=off

# ideally should be Maximum Segment Size
NGINX_POSTPONE_OUTPUT=1448

NGINX_SENDFILE_MAX_CHUNK=2m
NGINX_DIRECTIO_MIN_SIZE=8m

NGINX_THREAD_POOL_SIZE=32
NGINX_THREAD_POOL_MAX_QUEUE=65536

NGINX_OUTPUT_BUFFERS_NUM=2
NGINX_OUTPUT_BUFFERS_SIZE=32k

NGINX_OPEN_FILE_CACHE_MAX_SIZE=1000
NGINX_OPEN_FILE_CACHE_INACTIVE=1d
NGINX_OPEN_FILE_CACHE_MIN_USES=1
NGINX_OPEN_FILE_CACHE_VALID=60s

NGINX_UPSTREAM_KEEPALIVE_CONNECTIONS=32
NGINX_UPSTREAM_KEEPALIVE_REQUESTS=1000
NGINX_UPSTREAM_KEEPALIVE_TIME=1h
NGINX_UPSTREAM_KEEPALIVE_TIMEOUT=5m

NGINX_KEEPALIVE_REQUESTS=1000
NGINX_KEEPALIVE_TIME=1h
NGINX_KEEPALIVE_TIMEOUT=5m

NGINX_RESOLVER_TIMEOUT=10s

# and this one also default maybe?
NGINX_LINGERING_TIME=30s
NGINX_LINGERING_TIMEOUT=5s

NGINX_GZIP_COMP_LEVEL=5
NGINX_GZIP_NUM_BUFFERS=16
NGINX_GZIP_BUFFER_SIZE=8k
NGINX_GZIP_MIN_LENGTH=1000

NGINX_SSL_SESSION_TIMEOUT=1d
# 1m ~ 4000 sessions
NGINX_SSL_SESSION_CACHE_SIZE=10m

NGINX_UPSTREAM_ZONE_SIZE=64k

NGINX_PROXY_CONNECT_TIMEOUT=60s
NGINX_PROXY_READ_TIMEOUT=30s
NGINX_PROXY_SEND_TIMEOUT=60s

NGINX_PROXY_CACHE_LOCK_AGE=5s
# this one maybe should be changed from default
NGINX_PROXY_CACHE_LOCK_TIMEOUT=10s

# 1m ~ 8000 keys
NGINX_PROXY_CACHE_KEYS_ZONE_SIZE=10m
NGINX_PROXY_CACHE_INACTIVE=1d
NGINX_PROXY_CACHE_MAX_SIZE=10g

NGINX_LIMIT_RATE=1m
NGINX_LIMIT_RATE_AFTER=500k
# possibly decrease? note: "multiple hosts behind a Network Address Translation (NAT) device share the same IP address"
NGINX_LIMIT_CONN=100
# possibly increase?
NGINX_LIMIT_REQ_RATE=5r/s

NGINX_RETRY_AFTER=5

NGINX_AUTH_DELAY=1s
